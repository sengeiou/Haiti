  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-antrun-plugin</artifactId>
        <executions>
          <execution>
            <goals>
              <goal>run</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <tasks>
            <property name="runtime_classpath" refid="maven.runtime.classpath"/>
            <echo message="runtime classpath: ${runtime_classpath}"/>
            <java dir="${basedir}"
                  classname="com.aimir.fep.schedule.task.SetMSKToModem"
                  fork="true"
                  maxmemory="512M" >
              <jvmarg value="-server"/>
              <!-- <jvmarg value="-javaagent:${spring.instrument.path}/spring-instrument-${spring.version.4}.jar"/>   -->     
              <sysproperty key="name" value="MeterTimeSyncSoriaTask"/>
              <sysproperty key="file.encoding" value="UTF-8"/>
              <sysproperty key="log4j.configuration" value="log4j-MeterTimeSyncSoriaTask.xml"/>
              <classpath>
                <pathelement path=".:${runtime_classpath}:./lib/nuritelecom.jar:./lib/wolfssl.jar"/>
              </classpath>
            </java>
          </tasks>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.felix</groupId>
        <artifactId>maven-bundle-plugin</artifactId>
        <version>${maven-bundle-plugin.version}</version>
        <extensions>true</extensions>
      </plugin>
    </plugins>
  </build>
    
