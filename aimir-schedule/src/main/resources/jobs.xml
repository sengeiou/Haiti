  <job>
    <job-detail>
      <name>RecollectMeteringJob</name>
      <group>AimirJobScheduler</group>
      <description>Request recollect LP metering data of yesterday for empty LP</description>
      <job-class>nuri.aimir.scheduler.job.RecollectMeteringJob</job-class>
      <volatility>false</volatility>
      <durability>false</durability>
      <recover>false</recover>
      <job-data-map allows-transient-data="true">
        <entry>
          <key>retry</key>
          <value>10</value>
        </entry>
        <entry>
          <key>option.default</key>
          <value>0</value>
        </entry>
        <entry>
          <key>option.2</key>
          <value>16</value>
          <!-- GE:default(-1), selfread(0), current(1), previous(2), relay(3), event(10), measurement(12), demand(16) -->
        </entry>
        <entry>
          <key>savePeriod.default</key>
          <value>30</value>
        </entry>
        <entry>
          <key>savePeriod.2</key>
          <value>40</value>
        </entry>
      </job-data-map>
    </job-detail>
    <trigger>
      <cron>
        <name>RecollectMeteringTrigger</name>
        <group>AimirJobScheduler</group>
        <description>Request recollect LP metering data for empty LP</description>
        <job-name>RecollectMeteringJob</job-name>
        <job-group>AimirJobScheduler</job-group>
        <cron-expression>0 30 17 * * ?</cron-expression>
      </cron>
    </trigger>
  </job>
