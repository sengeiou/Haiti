  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>com.aimir</groupId>
    <artifactId>aimir-parent</artifactId>
    <version>3.3</version>
    <relativePath>../aimir-parent/pom.xml</relativePath>
  </parent>
  
  <artifactId>aimir-model-eclipselink</artifactId>
  <version>${aimir.version}</version>
  
  <build>
      <plugins>
        <!-- Java Compiler -->
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-compiler-plugin</artifactId>
          <configuration>
          <!-- 
            <source>1.8</source>
            <target>1.8</target>
             -->
            <encoding>UTF-8</encoding>
            <exclude>/src/main/java/init/*.java</exclude>
          </configuration>
        </plugin>
        <!-- init data -->
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-antrun-plugin</artifactId>
          <executions>
            <execution>
              <goals>
                <goal>run</goal>
              </goals>
            </execution>
          </executions>
          <configuration>
            <tasks>
              <property name="runtime_classpath" refid="maven.runtime.classpath"/>
              <echo message="runtime classpath: ${runtime_classpath}"/>
              <java dir="${basedir}"
                    classname="com.aimir.init.InitData"
                    fork="true"
                    maxmemory="512M"
                    >
                <!-- <jvmarg value="-javaagent:${eclipselink.path}/${eclipselink.version}/eclipselink-${eclipselink.version}.jar"/> -->
                <jvmarg value="-javaagent:${spring.instrument.path}/${spring.version.4}/spring-instrument-${spring.version.4}.jar"/>
                <sysproperty key="file.encoding" value="${file.encoding}"/>
                <sysproperty key="log4j.configuration" value="config/log4j.properties" />
                <arg value="${dataFile}"/>
                <classpath>
                  <pathelement path=".:${runtime_classpath}"/>
                </classpath>
              </java>
            </tasks>
          </configuration>
        </plugin>
      </plugins>
  </build>

