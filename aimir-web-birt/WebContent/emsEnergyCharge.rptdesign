        <text-data id="1147">
            <property name="fontFamily">"dotumche"</property>
            <property name="fontSize">24pt</property>
            <property name="fontWeight">bold</property>
            <property name="marginTop">0pt</property>
            <property name="marginBottom">40pt</property>
            <property name="textAlign">center</property>
            <expression name="valueExpr">
            	var yyyymm = params["searchDate"].value;&#13;
				var month = (yyyymm.substring(4)*1) + "월";&#13;
				&#13;
				var energyType="";&#13;
				if (params["energyType"].value == "EM") {&#13;
					energyType = "전기";&#13;
				} else if (params["energyType"].value == "GM") {&#13;
					energyType = "가스";&#13;
				} else if (params["energyType"].value == "WM") {&#13;
					energyType = "수도";&#13;
				} else if (params["energyType"].value == "HM") {&#13;
					energyType = "등유";&#13;
				}&#13;
				this.text = month + " " + energyType + "요금청구 금액내역";
			</expression>
            <property name="contentType">html</property>
        </text-data>
