<xs:element name="configID" minOccurs="0" maxOccurs="1" type="xs:string" sawsdl:modelReference="http://iec.ch/TC57/2010/CIM-schema-cim15#AssetFunction.configID">
<xs:annotation>
<xs:documentation>Configuration specified for this function.</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="direction" minOccurs="0" maxOccurs="1" type="m:ComDirectionKind" sawsdl:modelReference="http://iec.ch/TC57/2010/CIM-schema-cim15#ComFunction.direction">
<xs:annotation>
<xs:documentation>Kind of communication direction.</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="enabled" minOccurs="0" maxOccurs="1" type="xs:boolean" sawsdl:modelReference="http://iec.ch/TC57/2010/CIM-schema-cim15#EndDeviceFunction.enabled">
<xs:annotation>
<xs:documentation>True if the function is enabled.</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="enabledDateTime" minOccurs="0" maxOccurs="1" type="xs:dateTime" sawsdl:modelReference="http://landisgyr.com/v2/CIM-schema-ext#ComFunction.enabledDateTime">
<xs:annotation>
<xs:documentation>Date and time at which this com function became enabled.  Relevant in certain PAN networking scenarios whereby PAN communication is enabled starting at a particular date and time.</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="firmwareID" minOccurs="0" maxOccurs="1" type="xs:string" sawsdl:modelReference="http://iec.ch/TC57/2010/CIM-schema-cim15#AssetFunction.firmwareID">
