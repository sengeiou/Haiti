</xs:annotation>
<xs:restriction base="xs:float"/>
</xs:simpleType>
<xs:simpleType name="CurrentFlow" sawsdl:modelReference="http://iec.ch/TC57/2010/CIM-schema-cim15#CurrentFlow">
