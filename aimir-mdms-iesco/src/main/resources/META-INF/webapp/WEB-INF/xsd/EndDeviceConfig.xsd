<xs:annotation/>
</xs:element>
<xs:element name="Names" minOccurs="0" maxOccurs="unbounded" type="m:Name" sawsdl:modelReference="http://iec.ch/TC57/2010/CIM-schema-cim15#IdentifiedObject.Names">
<xs:annotation>
<xs:documentation>All names of this identified object.</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>
<xs:simpleType name="ComTechnologyKind" sawsdl:modelReference="http://iec.ch/TC57/2010/CIM-schema-cim15#ComTechnologyKind">
<xs:annotation>
<xs:documentation>Kind of communication technology.</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:enumeration value="cellular">
<xs:annotation>
<xs:documentation>Communicates using a public cellular radio network. A specific variant of 'rf'.</xs:documentation>
</xs:annotation>
</xs:enumeration>
<xs:enumeration value="echonetLite">
<xs:annotation>
<xs:documentation>Communicates based on the ECHONET Lite specification and conforming to the standards established by the ECHONET Consortium.</xs:documentation>
</xs:annotation>
</xs:enumeration>
<xs:enumeration value="ethernet">
<xs:annotation>
<xs:documentation>Communicates using one or more of a family of frame-based computer networking technologies conforming to the IEEE 802.3 standard.</xs:documentation>
</xs:annotation>
</xs:enumeration>
<xs:enumeration value="homePlug">
<xs:annotation>
<xs:documentation>Communicates using power line communication technologies conforming to the standards established by the HomePlug Powerline Alliance. A specific variant of 'plc'.</xs:documentation>
</xs:annotation>
</xs:enumeration>
<xs:enumeration value="pager">
<xs:annotation>
<xs:documentation>Communicates using a public one-way or two-way radio-based paging network. A specific variant of 'rf'.</xs:documentation>
</xs:annotation>
</xs:enumeration>
<xs:enumeration value="phone">
<xs:annotation>
<xs:documentation>Communicates using a basic, wireline telephone system.</xs:documentation>
</xs:annotation>
</xs:enumeration>
<xs:enumeration value="plc">
<xs:annotation>
<xs:documentation>Communicates using power line communication technologies.</xs:documentation>
</xs:annotation>
</xs:enumeration>
<xs:enumeration value="rf">
<xs:annotation>
<xs:documentation>Communicates using private or public radio-based technology.</xs:documentation>
</xs:annotation>
</xs:enumeration>
<xs:enumeration value="rfMesh">
<xs:annotation>
<xs:documentation>Communicates using a mesh radio technology. A specific variant of 'rf'.</xs:documentation>
</xs:annotation>
</xs:enumeration>
<xs:enumeration value="zigbee">
<xs:annotation>
<xs:documentation>Communicates using radio communication technologies conforming to the standards established by the ZigBee. A specific variant of 'rf'.</xs:documentation>
</xs:annotation>
</xs:enumeration>
</xs:restriction>
</xs:simpleType>
