<xs:element name="superCompressibilityCompensation" minOccurs="0" maxOccurs="1" type="xs:boolean" sawsdl:modelReference="http://iec.ch/TC57/2010/CIM-schema-cim15#EndDeviceCapability.superCompressibilityCompensation">
