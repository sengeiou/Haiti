<xs:documentation>Firmware version.</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="hardwareID" minOccurs="0" maxOccurs="1" type="xs:string" sawsdl:modelReference="http://iec.ch/TC57/2010/CIM-schema-cim15#AssetFunction.hardwareID">
