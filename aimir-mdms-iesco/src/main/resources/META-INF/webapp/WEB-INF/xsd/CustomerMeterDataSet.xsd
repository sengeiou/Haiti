<xs:sequence>
<xs:element name="autonomousDst" minOccurs="0" maxOccurs="1" type="xs:boolean" sawsdl:modelReference="http://iec.ch/TC57/2010/CIM-schema-cim15#EndDeviceCapability.autonomousDst">
