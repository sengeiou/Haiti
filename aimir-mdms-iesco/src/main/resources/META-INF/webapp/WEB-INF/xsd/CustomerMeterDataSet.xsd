</xs:annotation>
</xs:element>
<xs:element name="Batteries" minOccurs="0" maxOccurs="unbounded" type="m:Battery" sawsdl:modelReference="http://iec.ch/TC57/2010/CIM-schema-cim15#EndDevice.Batteries">
