<xs:documentation>Date the asset was received and first placed into inventory.</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="removalDate" minOccurs="0" maxOccurs="1" type="xs:date" sawsdl:modelReference="http://iec.ch/TC57/2010/CIM-schema-cim15#LifecycleDate.removalDate">
<xs:annotation>
